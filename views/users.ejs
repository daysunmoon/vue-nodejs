    <link rel="stylesheet" href="/css/common.css">
    <link rel="stylesheet" href="/css/banner.css">

</head>

<body>
    

    <div class="layout-main">
        <%- include('./common/header.ejs'); %>
        <%- include('./common/slide.ejs'); %>
        <div class="layout-body">
            <table class="banner-table">
                <thead>
                    <tr>
                        <th>序号</th>
                        <th>用户名</th>
                        <th>密码</th>
                        <th>昵称</th>
                        <th>是否为管理员</th>
                        <th>操作</th>
                    </tr>
                </thead>
                <tbody>
